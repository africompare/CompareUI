    <link rel="stylesheet" href="~/Content/vendor/animate.css/animate.css">
    <link rel="stylesheet" href="~/Content/vendor/whirl/dist/whirl.css">

    @*StyleBundle("~/Content/css")*@
    <link rel="stylesheet" href="~/Content/vendor/weather-icons/css/weather-icons.css">
    <link rel="stylesheet" href="~/Content/css/bootstrap.css">
    <link rel="stylesheet" href="~/Content/css/app.css">

    @*<link rel="stylesheet" href="~/Content/site.css" >*@
</head>
<body>
    <div class="wrapper">

        <!-- top navbar-->
        @await Html.PartialAsync("_TopHeader")
        <!--  sidebar nav-->
        @await Html.PartialAsync("_SideNav")

        @RenderBody()

        <footer class="footer-container mt-4"><span>&copy; 2020 - AfriCompare</span></footer>

    </div>








