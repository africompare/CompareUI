@{
    ViewBag.Title = "vendor My Subscribers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<section class="section-container">
    <!-- Page content-->
    <div class="content-wrapper">
        <div class="content-heading">
            <div>Tables<small>A showcase of different components inside tables</small></div>
        </div><!-- START card-->
        <div class="card card-default">
            <div class="card-header">Demo Table #1</div><!-- START table-responsive-->
            <div class="card-footer">
                <div class="d-flex">
                    <div>
                        <div class="input-group">
                            <input class="form-control" type="text" placeholder="Search">
                            <div class="input-group-append"><button class="btn btn-secondary" type="button">Button</button></div>
                        </div>
                    </div>
                    <div class="ml-auto">
                        <div class="input-group float-right">
                            <select class="custom-select" id="inputGroupSelect01">
                                <option value="0">Bulk action</option>
                                <option value="1">Delete</option>
                                <option value="2">Clone</option>
                                <option value="3">Export</option>
                            </select>
                            <div class="input-group-append"><button class="btn btn-secondary" type="button">Apply</button></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="table-responsive">
                <table class="table table-bordered table-hover" id="table-ext-1">
                    <thead>
                        <tr>
                            <th>UID</th>
                            <th>Picture</th>
                            <th>Username</th>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Email</th>
                            <th>Profile</th>
                            <th>Last Login</th>
                            <th data-check-all>
                                <div class="checkbox c-checkbox" data-toggle="tooltip" data-title="Check All"><label><input type="checkbox"><span class="fa fa-check"></span></label></div>
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>1</td>
                            <td>
                                <div class="media"><img class="img-fluid circle" src="img/user/01.jpg" alt="Image"></div>
                            </td>
                            <td>@@twitter</td>
                            <td>Larry</td>
                            <td>the Bird</td>
                            <td>mail@example.com</td>
                            <td class="text-center">
                                <div class="radial-bar radial-bar-25 radial-bar-xs" data-label="25%"></div>
                            </td>
                            <td>1 week</td>
                            <td>
                                <div class="checkbox c-checkbox"><label><input type="checkbox"><span class="fa fa-check"></span></label></div>
                            </td>
                        </tr>
                        <tr>
                            <td>2</td>
                            <td>
                                <div class="media"><img class="img-fluid circle" src="img/user/02.jpg" alt="Image"></div>
                            </td>
                            <td>@@mdo</td>
                            <td>Mark</td>
                            <td>Otto</td>
                            <td>mail@example.com</td>
                            <td class="text-center">
                                <div class="radial-bar radial-bar-50 radial-bar-xs" data-label="50%"></div>
                            </td>
                            <td>25 minutes</td>
                            <td>
                                <div class="checkbox c-checkbox"><label><input type="checkbox"><span class="fa fa-check"></span></label></div>
                            </td>
                        </tr>
                        <tr>
                            <td>3</td>
                            <td>
                                <div class="media"><img class="img-fluid circle" src="img/user/03.jpg" alt="Image"></div>
                            </td>
                            <td>@@fat</td>
                            <td>Jacob</td>
                            <td>Thornton</td>
                            <td>mail@example.com</td>
                            <td class="text-center">
                                <div class="radial-bar radial-bar-80 radial-bar-xs" data-label="80%"></div>
                            </td>
                            <td>10 hours</td>
                            <td>
                                <div class="checkbox c-checkbox"><label><input type="checkbox"><span class="fa fa-check"></span></label></div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div><!-- END table-responsive-->
        </div><!-- START card-->
    </div>
</section>